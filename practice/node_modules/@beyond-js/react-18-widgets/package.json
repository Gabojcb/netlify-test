{
  "name": "@beyond-js/react-18-widgets",
  "version": "1.0.1",
  "description": "Create web projects as standalone microfrontends with beyondJS and React 18",
  "license": "MIT",
  "libraries": {
    "imports": [
      "@beyond-js/kernel",
      "@beyond-js/widgets"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@beyond-js/widgets": "~0.1.5",
    "@beyond-js/kernel": "~0.1.9"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9"
  },
  "static": {},
  "exports": {
    "./base": {
      "types": "./base/base.d.ts",
      "amd": "./base/base.amd.js",
      "sjs": "./base/base.sjs.js",
      "browser": "./base/base.browser.mjs",
      "module": "./base/base.browser.mjs",
      "node": {
        "require": "./base/base.cjs.js",
        "import": "./base/base.mjs"
      }
    },
    "./hooks": {
      "types": "./hooks/hooks.d.ts",
      "amd": "./hooks/hooks.amd.js",
      "sjs": "./hooks/hooks.sjs.js",
      "browser": "./hooks/hooks.browser.mjs",
      "module": "./hooks/hooks.browser.mjs",
      "node": {
        "require": "./hooks/hooks.cjs.js",
        "import": "./hooks/hooks.mjs"
      }
    },
    "./page": {
      "types": "./page/page.d.ts",
      "amd": "./page/page.amd.js",
      "sjs": "./page/page.sjs.js",
      "browser": "./page/page.browser.mjs",
      "module": "./page/page.browser.mjs",
      "node": {
        "require": "./page/page.cjs.js",
        "import": "./page/page.mjs"
      }
    },
    "./config": {
      "amd": "./config/config.amd.js",
      "sjs": "./config/config.sjs.js",
      "browser": "./config/config.browser.mjs",
      "module": "./config/config.browser.mjs",
      "node": {
        "import": "./config/config.mjs",
        "require": "./config/config.cjs.js"
      }
    }
  },
  "uimport": [
    "react",
    "react-dom/client",
    "react-dom/server"
  ]
}